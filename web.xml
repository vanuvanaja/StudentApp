  	<servlet-class>com.jspiders.servlets.ResetPasswordServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ResetPasswordServlet</servlet-name>
  	<url-pattern>/resetpassword</url-pattern>
  </servlet-mapping>
</web-app>









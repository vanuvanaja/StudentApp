  	<servlet-name>update</servlet-name>
  	<url-pattern>/update</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>BodyPage</servlet-name>
  	<servlet-class>com.jspiders.servlets.BodyPageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>BodyPage</servlet-name>
  	<url-pattern>/bodyPage</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>LoginPage</servlet-name>
  	<servlet-class>com.jspiders.servlets.LoginPageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginPage</servlet-name>
  	<url-pattern>/loginpage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginPage</servlet-name>
  	<url-pattern>/loginPage</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>graingersearch</servlet-name>

  </servlet>
  <servlet-mapping>
  	<servlet-name>changepassword</servlet-name>
  	<url-pattern>/changepassword</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>editstudent</servlet-name>
  	<servlet-class>com.jspiders.servlets.EditStudentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>editstudent</servlet-name>
  	<url-pattern>/edit</url-pattern>
  </servlet-mapping>
  
  <servlet>
